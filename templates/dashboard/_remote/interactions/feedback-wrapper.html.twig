<div class="row g-4">
	{% for feedback in websiteData.websiteConfig.event.feedback %}
		<div class="col-md-6">
			<div class="card interaction-card card shadow-sm">
				<div class="card-header m-0 d-flex justify-content-between align-items-center">
					{% if feedback.user %}
						{{ feedback.user.firstname }}
						{{ feedback.user.lastname }}<br>
						{{ feedback.user.email }}
						-
						{{ feedback.user.telephone }}
					{% else %}
						Feedback anonyme
					{% endif %}
					<span>
						Note :
						{{ feedback.stars }}/5
					</span>
				</div>
				<div class="card-body">
					<blockquote class="blockquote mb-0">
						<p>{{ feedback.message }}</p>
					</blockquote>
				</div>
			</div>
		</div>
	{% endfor %}
</div>
